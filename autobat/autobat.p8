function debug_str(o)
    if type(o) == 'table' then
        local s = "{\n"
        for k,v in pairs(o) do
            s ..= ' ' .. tostr(k) .. ' = ' .. debug_str(v) .. "\n"
        end
        return s.."}"
    else
        return tostr(o)
    end
end

function debug(o)
    s = debug_str(o)
    printh(s, "test.txt")
    print(s)
end
